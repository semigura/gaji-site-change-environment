{
  "name": "gaji-sitee",
  "repository": "git@github.com:Gaji-Labo/gaji-site.git",
  "private": true,
  "engines": {
    "node": ">=10.13.0 <11.0"
  },
  "scripts": {
    "dev": "yarn && yarn svg-sprite && gatsby develop",
    "build": "yarn && yarn svg-sprite && gatsby build",
    "start": "gatsby develop",
    "lint": "yarn lint:js && yarn lint:css",
    "fixlint": "yarn fixlint:js && yarn fixlint:css",
    "test": "yarn && yarn svg-sprite && yarn jest",
    "storybook": "yarn && yarn svg-sprite && start-storybook -s ./public -p 6006",
    "svg-sprite": "svg-sprite -C svg-sprite.json --shape-transform-svgo svgo.json src/static/icons/*.svg",
    "lint:js": "eslint --ext .jsx --ext .js src/",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "fixlint:js": "yarn prettier:js && yarn lint:js --fix",
    "fixlint:css": "yarn prettier:css && stylelint \"src/**/*.scss\" --fix --syntax scss",
    "prettier:js": "yarn prettier --write src/**/*.{js,jsx}",
    "prettier:css": "yarn prettier --write src/**/*.scss"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn fixlint:js",
      "git add"
    ],
    "src/**/*.{scss}": [
      "yarn fixlint:css",
      "git add"
    ]
  },
  "dependencies": {
    "autoprefixer": "^9.8.0",
    "classnames": "^2.2.6",
    "css-mqpacker": "^7.0.0",
    "gatsby": "^2.22.12",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-buildtime-timezone": "^1.0.3",
    "gatsby-plugin-catch-links": "^2.3.3",
    "gatsby-plugin-draft": "^0.0.5",
    "gatsby-plugin-exclude": "^1.0.2",
    "gatsby-plugin-google-analytics": "^2.3.2",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-netlify": "^2.3.3",
    "gatsby-plugin-netlify-cms": "^4.3.3",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-sass": "^2.3.2",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-plugin-sitemap": "^2.4.3",
    "gatsby-remark-copy-images": "^0.2.1",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-figure": "^1.0.0",
    "gatsby-remark-figure-caption": "^2.0.0",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-transformer-remark": "^2.8.13",
    "gatsby-transformer-sharp": "^2.5.3",
    "markdown-it": "^11.0.0",
    "netlify-cms": "^2.10.49",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-highlight": "^0.12.0",
    "react-scripts": "^3.4.1",
    "react-scroll": "^1.7.16",
    "react-slidedown": "^2.4.5",
    "sanitize.css": "^11.0.1",
    "showdown": "^1.9.1",
    "svg-sprite": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-options": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.4",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-gatsby": "^0.4.7",
    "backstopjs": "^5.0.1",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.2.5",
    "hygen": "^5.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.2.7",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "sass-loader": "^8.0.2",
    "sass-resources-loader": "^2.0.3",
    "style-loader": "^1.2.1",
    "stylelint": "^13.5.0",
    "stylelint-config-property-sort-order-smacss": "^6.3.0",
    "stylelint-config-standard": "^20.0.0",
    "url-loader": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn svg-sprite && yarn lint"
    }
  }
}
